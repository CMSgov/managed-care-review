    "devDependencies": {
        "@cypress-audit/pa11y": "^1.3.0",
        "chromedriver": "^103.0.0",
        "cypress": "^9.1.1",
        "cypress-pipe": "^2.0.0",
        "husky": "^8.0.1",
        "lerna": "^4.0.0",
        "lint-staged": "^13.0.1",
