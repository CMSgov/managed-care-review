<b>Submitted by:</b> <%= it.resubmittedBy %><br />
<b>Updated on:</b> <%= it.resubmittedOn %><br />
<b>Changes made:</b> <%= it.resubmissionReason %><br />
<% if (it.shouldIncludeRates) { %>
<b>Rate names</b>:
<ul>
<% it.rateInfos.forEach(function(rate){ %>
<li><%= rate.rateName %></li>
<% }) %>
</ul>
<% } %>
