<b>Unlocked by:</b> <%= it.unlockedBy %><br />
<b>Unlocked on:</b> <%= it.unlockedOn %><br />
<b>Reason for unlock:</b> <%= it.unlockedReason %><br />
<% if (it.shouldIncludeRates) { %>
<b>Rate names</b>:
<ul>
<% it.rateInfos.forEach(function(rate){ %>
<li><%= rate.rateName %></li>
<% }) %>
</ul>
<% } %>
