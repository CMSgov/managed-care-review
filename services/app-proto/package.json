{
    "name": "app-proto",
    "private": "true",
    "version": "1.0.0",
    "description": "app-proto Typescript definitions and helpers",
    "scripts": {
        "generate-code": "yarn protogen",
        "protogen": "yarn proto:compile && yarn proto:ts",
        "protogen:watch": "find src | entr yarn protogen",
        "proto:compile": "rm -rf gen && mkdir gen && pbjs -t static-module -w commonjs -o ../app-web/src/gen/stateSubmissionProto.js src/state_submission.proto",
        "proto:ts": "pbts -o ../app-web/src/gen/stateSubmissionProto.d.ts ../app-web/src/gen/stateSubmissionProto.js",
        "precommit": "lint-staged",
        "lint": "protolint lint ."
    },
    "lint-staged": {
        "*.proto": [
            "protolint lint ."
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "protobufjs": "^6.11.2"
    },
    "devDependencies": {
        "lint-staged": "^12.1.2"
    }
}
