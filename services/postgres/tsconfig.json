{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "module": "es2022",
        "lib": ["es2022"],
        "target": "es2022",
        "moduleResolution": "node",

        "baseUrl": "/",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": ["scripts"]
}
