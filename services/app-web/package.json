{
    "name": "app-web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.4.15",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^12.1.0",
        "@testing-library/user-event": "^13.5.0",
        "@trussworks/react-uswds": "^2.2.0",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^27.0.1",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.9",
        "@types/yup": "^0.29.11",
        "aws-amplify": "^4.3.0",
        "axios": "^0.21.2",
        "dayjs": "^1.10.5",
        "formik": "^2.2.9",
        "graphql": "^15.5.1",
        "react": "^17.0.2",
        "react-aria-modal": "^4.0.0",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.0",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "sass": "^1.43.3",
        "typescript": "^4.0.3",
        "url-parse": "^1.5.2",
        "uswds": "^2.12.1",
        "uuid": "^8.3.2",
        "web-vitals": "^2.1.0",
        "yup": "^0.32.9",
        "zod": "^3.10.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -rf node_modules && yarn cache clean",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint src/",
        "precommit": "tsc --noEmit && lint-staged",
        "test:a11y": "pa11y-ci",
        "test:once": "react-scripts test --watchAll=false",
        "test:update": "react-scripts test -u",
        "storybook": "start-storybook -p 6006 -s public --quiet",
        "build-storybook": "build-storybook -s public --quiet"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write src/**/*.ts{,x}"
        ]
    },
    "resolutions": {
        "@storybook/react/babel-loader": "8.1.0"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "src/pages/Auth/Signup",
            "src/pages/Auth/ConfirmSignUp"
        ],
        "coverageReporters": [
            [
                "lcov",
                {
                    "projectRoot": "../../"
                }
            ],
            "text"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.3.7",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.2.9",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-test-renderer": "^17.0.1",
        "@types/url-parse": "^1.4.3",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.31.2",
        "classnames": "^2.2.6",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "graphql.macro": "^1.4.2",
        "lint-staged": "^11.2.6",
        "pa11y-ci": "^2.4.2",
        "prettier": "^2.3.2",
        "react-test-renderer": "^17.0.2",
        "serverless": "^2.62.0",
        "serverless-cloudfront-invalidate": "^1.11.0",
        "serverless-plugin-scripts": "^1.0.2",
        "serverless-s3-sync": "^1.14.4",
        "serverless-stack-termination-protection": "^1.0.4"
    }
}
