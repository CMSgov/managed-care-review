    "@chromatic-com/storybook": "^1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@peculiar/webcrypto": "^1.5.0",
    "@storybook/addon-a11y": "^8.1.7",
    "@storybook/addon-essentials": "^8.1.7",
    "@storybook/addon-links": "^8.1.7",
    "@storybook/react": "^8.1.7",
    "@storybook/react-vite": "^8.1.7",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.4.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^6.5.0",
    "add": "^2.0.6",
    "classnames": "^2.2.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "graphql.macro": "^1.4.2",
    "jest-launchdarkly-mock": "^2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.1.0",
    "react-select-event": "^5.5.0",
    "react-test-renderer": "^18.2.0",
    "serverless": "^3.26.0",
    "serverless-cf-invalidate-proxy": "^1.0.1",
    "serverless-s3-sync": "^3.0.0",
    "serverless-stack-termination-protection": "^2.0.2",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "vite-plugin-commonjs": "^0.10.1",
    "vite-plugin-graphql-loader": "^3.0.1",
