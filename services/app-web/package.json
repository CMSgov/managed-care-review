{
  "name": "app-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "precommit": "lint-staged",
    "start": "vite",
    "build": "tsc && vite build",
    "clean": "rm -rf node_modules && yarn cache clean",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/ --max-warnings=0 && yarn lint:style",
    "lint:style": "npx stylelint '**/*.scss'",
    "test": "TZ=UTC vitest",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006 -s public --quiet",
    "build-storybook": "storybook build -s public --quiet",
    "prettier": "npx prettier -u --write \"**/*.+(ts|tsx|json)\""
  },
  "lint-staged": {
    "*.{js,jsx, ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "**/*.scss": [
      "yarn lint:style",
      "prettier --write"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/*.stories.tsx",
      "src/s3",
      "src/api",
      "src/testHelpers",
      "src/localAuth",
      "src/gen",
      "src/components/Header/StateIcon/StateIcon.tsx",
      "src/pages/Auth/CheckAuth.tsx",
      "src/pages/Auth/CognitoLogin.tsx",
      "src/pages/Auth/Signup",
      "src/pages/Auth/ConfirmSignUp",
      "src/pages/Help/Help.tsx",
      "App.tsx",
      "index.ts",
      "reportWebVitals.ts"
    ],
    "coverageReporters": [
      [
        "json",
        {
          "projectRoot": "../../"
        }
      ],
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@apollo/explorer": "^3.0.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-web": "^0.39.0",
    "@opentelemetry/context-zone": "^1.24.1",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
    "@opentelemetry/id-generator-aws-xray": "^1.2.1",
    "@opentelemetry/instrumentation": "^0.52.0",
    "@opentelemetry/instrumentation-document-load": "^0.34.0",
    "@opentelemetry/instrumentation-fetch": "^0.45.1",
    "@opentelemetry/propagator-aws-xray": "^1.3.1",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-trace-web": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@tanstack/react-table": "^8.8.5",
    "@trussworks/react-uswds": "^4.0.0",
    "@types/path-browserify": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "aws-amplify": "^5.0.10",
    "axios": "^1.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "dayjs": "^1.10.5",
    "formik": "^2.2.9",
    "graphql": "^16.8.1",
    "jotai": "^2.2.1",
    "jotai-location": "^0.5.1",
    "jsdom": "^24.1.0",
    "launchdarkly-react-client-sdk": "^3.0.10",
    "path-browserify": "^1.0.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-router-dom": "^6.5.0",
    "react-select": "^5.7.7",
    "sass": "^1.49.11",
    "typescript": "^4.4.4",
    "url-parse": "^1.5.2",
    "use-deep-compare-effect": "^1.8.1",
    "uswds": "^2.13.1",
    "uuid": "^9.0.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^3.0.1",
    "yup": "^0.32.9",
    "zod": "^3.10.1"
  },
  "devDependencies": {
    "@aws-sdk/client-amplify": "^3.529.1",
    "@aws-sdk/client-s3": "^3.529.1",
