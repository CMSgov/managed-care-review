{
    "name": "app-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "precommit": "lint-staged",
        "start": "react-scripts -r @cypress/instrument-cra start",
        "build": "sh ../../run_build.sh",
        "build_prod": "react scripts build",
        "build_instrumented": "CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -rf node_modules && yarn cache clean",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint src/ --max-warnings=0",
        "test:once": "react-scripts test --watchAll=false --runInBand --ci",
        "test:update": "react-scripts test -u",
        "storybook": "start-storybook -p 6006 -s public --quiet",
        "build-storybook": "build-storybook -s public --quiet"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --max-warnings=0",
            "prettier --write"
        ]
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "/*.stories.tsx",
            "src/s3",
            "src/api",
            "src/testHelpers",
            "src/localAuth",
            "src/gen",
            "src/components/Header/StateIcon/StateIcon.tsx",
            "src/pages/Auth/CheckAuth.tsx",
            "src/pages/Auth/CognitoLogin.tsx",
            "src/pages/Auth/Signup",
            "src/pages/Auth/ConfirmSignUp",
            "src/pages/Help/Help.tsx",
            "App.tsx",
            "index.ts",
            "log_event.ts",
            "reportWebVitals.ts"
        ],
        "coverageReporters": [
            [
                "json",
                {
                    "projectRoot": "../../"
                }
            ],
            [
                "lcov",
                {
                    "projectRoot": "../../"
                }
            ],
            "text"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.4.15",
        "@cypress/code-coverage": "^3.10.0",
        "@cypress/instrument-cra": "^1.4.0",
        "@opentelemetry/api": "^1.1.0",
        "@opentelemetry/auto-instrumentations-web": "^0.28.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.28.0",
        "@opentelemetry/id-generator-aws-xray": "^1.1.0",
        "@opentelemetry/instrumentation": "^0.28.0",
        "@opentelemetry/instrumentation-document-load": "^0.28.0",
        "@opentelemetry/instrumentation-fetch": "^0.28.0",
        "@opentelemetry/propagator-aws-xray": "^1.1.0",
        "@opentelemetry/resources": "^1.2.0",
        "@opentelemetry/sdk-trace-base": "^1.2.0",
        "@opentelemetry/sdk-trace-web": "^1.2.0",
        "@opentelemetry/semantic-conventions": "^1.2.0",
        "@trussworks/react-uswds": "^2.8.0",
        "@types/path-browserify": "^1.0.0",
        "aws-amplify": "^4.3.4",
        "axios": "^0.25.0",
        "babel-plugin-istanbul": "^6.1.1",
        "dayjs": "^1.10.5",
        "formik": "^2.2.9",
        "graphql": "^16.2.0",
        "launchdarkly-react-client-sdk": "^2.25.2",
        "path-browserify": "^1.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^3.1.0",
        "react-router-dom": "^6.3.0",
        "react-select": "^5.3.2",
        "sass": "^1.49.11",
        "typescript": "^4.4.4",
        "url-parse": "^1.5.2",
        "uswds": "^2.13.1",
        "uuid": "^8.3.2",
        "web-vitals": "^2.1.2",
        "yup": "^0.32.9",
        "zod": "^3.10.1"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^6.5.4",
        "@storybook/addon-essentials": "^6.5.4",
        "@storybook/addon-links": "^6.5.4",
        "@storybook/builder-webpack5": "^6.5.4",
        "@storybook/manager-webpack5": "^6.5.4",
        "@storybook/preset-create-react-app": "4.1.2",
        "@storybook/react": "^6.5.4",
        "@testing-library/cypress": "latest",
        "@testing-library/jest-dom": "latest",
        "@testing-library/react": "latest",
        "@testing-library/user-event": "latest",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^27.0.1",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-router-dom": "^5.3.0",
        "@types/react-test-renderer": "^18.0.0",
        "@types/url-parse": "^1.4.3",
        "@types/uuid": "^8.3.1",
        "@types/yup": "^0.29.11",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "classnames": "^2.2.6",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-jest": "^26.6.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "graphql.macro": "^1.4.2",
        "jest-launchdarkly-mock": "^1.0.6",
        "lint-staged": "^13.0.1",
        "prettier": "^2.3.2",
        "react-scripts": "5.0.1",
        "react-select-event": "^5.5.0",
        "react-test-renderer": "^18.2.0",
        "serverless": "^3.19.0",
        "serverless-cloudfront-invalidate": "^1.11.0",
        "serverless-s3-sync": "^3.0.0",
        "serverless-stack-termination-protection": "^2.0.2",
        "webpack": "^5.72.0"
    },
    "resolutions": {
        "react-router-dom": "5.3.0",
        "@types/react-router-dom": "5.3.3"
    }
}
